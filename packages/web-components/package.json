{
  "name": "@dctjs/web-components",
  "version": "1.0.1",
  "description": "Basic Web Components",
  "keywords": [
    "app",
    "components",
    "hybrid",
    "webapp",
    "progressive web app",
    "pwa",
    "react",
    "stenciljs"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/dct-components/dct-components.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidctaylor/web-components.git"
  },
  "author": "David Taylor",
  "homepage": "https://github.com/davidctaylor/web-components#readme",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build-prod": "stencil build --docs --prod",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006 --static-dir ../images",
    "build-storybook": "storybook build --static-dir images"
  },
  "dependencies": {
    "@stencil/core": "^4.7.0",
    "@stencil/sass": "^3.0.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@stencil/react-output-target": "^0.5.3",
    "@storybook/addon-docs": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/html": "^7.5.3",
    "@storybook/html-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/web-components": "^7.5.3",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "21.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3"
  },
  "license": "MIT"
}
